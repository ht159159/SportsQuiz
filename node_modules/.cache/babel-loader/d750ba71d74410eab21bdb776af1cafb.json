Category: function changeCategory(index) {\n      this.categoryType = index;\n    },\n    // 類別選擇- 呼叫API\n    getMedalRank: function getMedalRank(index) {\n      var _this = this;\n\n      this.imgAry = [];\n      var data = {\n        type: index\n      };\n      apiGetRankingMedal(data, true).then(function (res) {\n        _this.imgData = res; // 把陣列變成兩個一包\n\n        for (var i = 0; i < _this.imgData.length; i += 2) {\n          _this.imgAry.push(_this.imgData.slice(i, i + 2));\n        }\n      });\n    },\n    // 彈窗\n    showPopup: function showPopup() {\n      this.ShowExplain = true;\n    }\n  }\n};",{"version":3,"sources":["MedalDisplay.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8FA,OAAA,IAAA,MAAA,8BAAA;AACA,SAAA,kBAAA,QAAA,iBAAA;AACA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,IAAA,EAAA;AADA,GADA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,CAAA,KAAA,EAAA,CAAA,UAAA,CAAA,EAAA,KAAA,EAAA,CAAA,cAAA,CAAA,EAAA,KAAA,EAAA,CAAA,mBAAA,CAAA,CADA;AACA;AACA,MAAA,YAAA,EAAA,CAFA;AAGA,MAAA,MAAA,EAAA,EAHA;AAIA,MAAA,OAAA,EAAA,EAJA;AAKA,MAAA,WAAA,EAAA;AALA,KAAA;AAOA,GAZA;AAcA,EAAA,OAdA,qBAcA;AACA;AACA,SAAA,YAAA,CAAA,KAAA,YAAA,EAFA,CAIA;;AACA,SAAA,cAAA,CAAA,KAAA,YAAA;AACA,GApBA;AAsBA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,cAFA,0BAEA,KAFA,EAEA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAJA;AAMA;AACA,IAAA,YAPA,wBAOA,KAPA,EAOA;AAAA;;AACA,WAAA,MAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,MAAA,kBAAA,CAAA,IAAA,EAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CADA,CAEA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,OAAA,CAAA,MAAA,EAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,GAAA,CAAA,CAAA;AACA;AACA,OANA;AAOA,KAnBA;AAqBA;AACA,IAAA,SAtBA,uBAsBA;AACA,WAAA,WAAA,GAAA,IAAA;AACA;AAxBA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"medal-wrap\">\r\n    <main class=\"medal\">\r\n      <!-- 彈窗 -->\r\n      <div class=\"explain\" v-if=\"ShowExplain\">\r\n        <i class=\"explain-x\" @click=\"ShowExplain = !ShowExplain\"></i>\r\n        <p class=\"rank-popup-title\">{{ $t(\"MedalDescription\") }}</p>  <!-- 【 勋章说明 】 -->\r\n        <div class=\"rank-popup-content\">\r\n          <div class=\"text-wrap\">\r\n            <p>\r\n              <span class=\"green-title\">{{ $t(\"BetMedal\") }}</span>  <!-- 【 投注勋章 】 -->\r\n              ：{{ $t(\"BetMedalTip\") }}\r\n            </p>\r\n            <p>\r\n              <span class=\"green-title\">{{ $t(\"MedalVictory\") }}</span>  <!-- 【 胜次勋章 】 -->\r\n              ：{{ $t(\"MedalVictoryTip\") }}  <!-- 【 根据用户在普通投注内的投注胜利次数，计算排行榜，胜利次数最高的用户获得第一名勋章。以此类推 】 -->\r\n            </p>\r\n            <p>\r\n              <span class=\"green-title\">{{ $t(\"MedalWinningCoins\") }}</span>  <!-- 【 赢币勋章 】 -->\r\n              ：{{ $t(\"MedalWinningCoinsTip\") }}\r\n            </p>\r\n            <span class=\"blue-text\">{{ $t(\"UpdateCycle\") }}：{{ $t(\"UpdateTime\") }}</span>  <!-- 【 更新周期 】 -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 彈窗END -->\r\n\r\n      <div class=\"medal-header\">\r\n        <back class=\"back-icon img-he\"></back>\r\n        <h3>{{  $t('MedalDisplay') }}</h3>  <!-- 【 勋章展示 】 -->\r\n      </div>\r\n      <div class=\"main-content-wrap\">\r\n        <div class=\"banner\"></div>\r\n        <div class=\"category-wrap\">\r\n          <div\r\n            class=\"category\"\r\n            v-for=\"(item, index) in category\"\r\n            :key=\"index\"\r\n            @click=\"\r\n              changeCategory(index);\r\n              getMedalRank(index);\r\n            \"\r\n            :class=\"{ 'category-focus': categoryType === index }\"\r\n          >\r\n            <span>{{ item }}</span>\r\n          </div>\r\n          <!-- 彈窗按鈕 -->\r\n          <div class=\"tip\" @click=\"showPopup\"></div>\r\n        </div>\r\n        <div class=\"content\">\r\n          <div class=\"medal-wrap\">\r\n            <div class=\"medal-category\">\r\n              <div\r\n                class=\"category-block\"\r\n                v-for=\"(item, index) in imgAry\"\r\n                :key=\"index\"\r\n              >\r\n                <div\r\n                  class=\"category-content-wrap \"\r\n                  v-for=\"(pic, i) in item\"\r\n                  :key=\"i\"\r\n                >\r\n                  <img\r\n                    :class=\"[pic.IsShow ? 'iscolorful' : 'isGray']\"\r\n                    :src=\"pic.ImageUrl\"\r\n                    alt=\"\"\r\n                  />\r\n                  <!-- 進度條 -->\r\n                  <el-slider\r\n                    v-show=\"pic.IsBar\"\r\n                    v-model=\"pic.Amount\"\r\n                    :show-tooltip=\"false\"\r\n                    :min=\"pic.MinValue\"\r\n                    :max=\"pic.MaxValue\"\r\n                    :class=\"[pic.IsBar ? 'iscolorful' : 'isGray']\"\r\n                    disabled=\"disabled\"\r\n                  ></el-slider>\r\n                  <div\r\n                    v-show=\"pic.IsBar\"\r\n                    class=\"bar-value\"\r\n                    :class=\"[pic.IsBar ? 'iscolorful' : 'isGray']\"\r\n                  >\r\n                    {{ pic.Amount }} / {{ pic.MaxValue }}\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n<script>\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport { apiGetRankingMedal } from \"@/api/Rankboard\";\r\nexport default {\r\n  components: {\r\n    back\r\n  },\r\n  data() {\r\n    return {\r\n      category: [this.$t('BetMedal'), this.$t('MedalVictory'), this.$t('MedalWinningCoins')],  // 【 投注勋章 】【 胜次勋章 】【 赢币勋章 】\r\n      categoryType: 0,\r\n      imgAry: [],\r\n      imgData: [],\r\n      ShowExplain: false\r\n    };\r\n  },\r\n\r\n  created() {\r\n    // 類別選擇- 呼叫API\r\n    this.getMedalRank(this.categoryType);\r\n\r\n    //類別選擇-切換畫面顯示\r\n    this.changeCategory(this.categoryType);\r\n  },\r\n\r\n  methods: {\r\n    //類別選擇-切換畫面顯示\r\n    changeCategory(index) {\r\n      this.categoryType = index;\r\n    },\r\n\r\n    // 類別選擇- 呼叫API\r\n    getMedalRank(index) {\r\n      this.imgAry = [];\r\n      let data = {\r\n        type: index\r\n      };\r\n      apiGetRankingMedal(data, true).then(res => {\r\n        this.imgData = res;\r\n        // 把陣列變成兩個一包\r\n        for (let i = 0; i < this.imgData.length; i += 2) {\r\n          this.imgAry.push(this.imgData.slice(i, i + 2));\r\n        }\r\n      });\r\n    },\r\n\r\n    // 彈窗\r\n    showPopup() {\r\n      this.ShowExplain = true;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/personalCenter"}]}