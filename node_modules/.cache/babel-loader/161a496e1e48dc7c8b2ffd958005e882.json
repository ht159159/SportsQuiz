</div>\r\n            </div>\r\n          </div>\r\n          <!-- 會員 -->\r\n          <div class=\"M-message-list\" v-for=\"(item, index) in clubMsgList\" :key=\"index\" v-if=\"isManagement == 2 || isManagement == 3\"  @click=\"isOpen(index)\">\r\n            <div class=\"M-message-list-up\">\r\n              <img src=\"@/assets/club/message.png\">\r\n              <div class=\"M-message-list-up-right\">\r\n                <div class=\"M-message-list-wrap\">\r\n                  <div class=\"M-message-list-title\">{{ item.Title }}</div>\r\n                  <div class=\"M-message-list-date\">{{ item.CreateTime }}</div>\r\n                </div>\r\n                <div class=\"M-message-list-detail\" :class=\"{'open-msg': detailIndex == index && isOpenDetail}\">{{ item.Content }}</div>\r\n              </div>\r\n            </div>\r\n            <div class=\"M-message-list-down\">{{ $t(\"SeeDetails\") }}</div>\r\n          </div>\r\n        </div>\r\n        \r\n        <!-- 發佈頁面 -->\r\n        <div class=\"release\" v-if=\"isRelease == true\">123</div>\r\n        <!-- 無資料時 -->\r\n        <div class=\"noData\" v-if=\"clubMsgList.length == 0\">\r\n          <p>{{ $t(\"NoData\") }}</p>  <!-- 【 暂无数据 】 -->\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 俱樂部活動 -->\r\n      <div class=\"club-activity-wrap\" v-if=\"categoryIndex == 1\">11111111</div>\r\n\r\n      <!-- 俱樂部公告 -->\r\n      <div class=\"club-bulletin-wrap\" v-if=\"categoryIndex == 2\">22222222222</div>\r\n    </div>\r\n\r\n    <div class=\"release-btn-wrap\" v-if=\"isManagement == 1 && isRelease == false\">\r\n      <div class=\"release-btn\" @click=\"openRelease()\">{{ $t('Release') }}</div><!-- 发布 -->\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport { apiGetClubAnnouncement } from \"@/api/Club.js\";\r\nexport default {\r\n  components: {\r\n    back,\r\n  },\r\n  data() {\r\n    return {\r\n    // 俱乐部信息; 俱乐部活动; 俱乐部公告;\r\n    category: [\