           <!--投注-->\r\n            <div class=\"result-box-content-text\">\r\n              <span>{{ $t(\"Bet\") }}:</span>\r\n              <p>{{ item.BettedAmount }}</p>\r\n            </div>\r\n\r\n            <!--下注時間-->\r\n            <div class=\"result-box-content-text\">\r\n              <span>{{ $t(\"BettingTime\") }}:</span>\r\n              <p>\r\n                {{ item.CreateTime.slice(0, item.CreateTime.indexOf(\"T\")) }}\r\n              </p>\r\n            </div>\r\n\r\n            <!--盈利-->\r\n            <div\r\n              class=\"result-box-content-text\"\r\n              v-if=\"item.SettlementStatus == 1\"\r\n            >\r\n              <span>{{ $t('Profit') }}:</span>\r\n              <p class=\"red\">{{ item.WinIntegral }}</p>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"result-box-footer\">\r\n            <div class=\"result-box-footer-result\">\r\n              <!--結果-->\r\n              <span>{{ $t(\"MatchResults\") }}:</span>\r\n              <!--未结算-->\r\n              <p class=\"blue\" v-show=\"item.SettlementStatus == 0\">\r\n                {{ $t(\"Unsettlement\") }}\r\n              </p>\r\n\r\n              <!--勝-->\r\n              <p\r\n                class=\"green\"\r\n                v-show=\"item.SettlementStatus == 2 && item.WinIntegral > 0\"\r\n              >\r\n                {{ $t(\"Win\") }}\r\n              </p>\r\n\r\n              <!--負-->\r\n              <p\r\n                class=\"green\"\r\n                v-show=\"item.SettlementStatus == 2 && item.WinIntegral < 0\"\r\n              >\r\n                {{ $t(\"Lose\") }}\r\n              </p>\r\n\r\n              <!--已结算-->\r\n              <p class=\"green\" v-show=\"item.SettlementStatus == 2\">{{ $t('Closed') }}</p>\r\n            </div>\r\n          </div>\r\n          <!-- 已結算 -->\r\n            <!-- 簡中版 -->\r\n          <div v-if=\"lang == 'zh-CN'\" :class=\"[item.SettlementStatus == 2 && item.WinIntegral > 0? 'have-result' : 'no-result']\"></div>\r\n            <!-- 英文版 -->\r\n          <div v-if=\"lang == 'en-US'\" :class=\"[item.SettlementStatus == 2 && item.WinIntegral > 0? 'have-result-US' : 'no-result-US']\"></div>\r\n            <!-- 繁中版 -->\r\n          <div v-if=\"lang == 'zh-TW'\" :class=\"[item.SettlementStatus == 2  && item.WinIntegral > 0? 'have-result' : 'no-result']\"></div>\r\n\r\n          <!-- 失效 -->\r\n            <!-- 簡中版 -->\r\n          <div v-if=\"lang == 'zh-CN' && item.SettlementStatus == 2 && item.WinIntegral == 0\" class=\"invalid-CN\"></div>\r\n            <!-- 英文版 -->\r\n          <div v-if=\"lang == 'en-US' && item.SettlementStatus == 2 && item.WinIntegral == 0\" class=\"invalid-US\"></div>\r\n            <!-- 繁中版 -->\r\n          <div v-if=\"lang == 'zh-TW' && item.SettlementStatus == 2 && item.WinIntegral == 0\" class=\"invalid-TW\"></div>\r\n        </div>\r\n        <div style=\"font-size: .24rem;text-align:center;line-height: .48rem\" v-if=\"isLoading\">\r\n          {{ $t('WipeLoading') }}...  <!-- 【 加载中… 】 -->\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"no-Data\" v-if=\"isNoData\">\r\n        <p>{{ $t(\"NoData\") }}</p>  <!-- 【 暂无数据 】 -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport {\r\n  apiGetClubGamblingBetResultList,\r\n  apiGetClubGamblingBetResult\r\n} from \"@/api/ClubGamblings.js\";\r\n\r\nexport default {\r\n  components: {\r\n    back\r\n  },\r\n  data() {\r\n    return {\r\n      lang: \"\",\r\n      resultData: [],\r\n      betId: \"\",\r\n      isLoading: false,\r\n      isNoData: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.lang = JSON.parse(localStorage.getItem(\"lang\")).tag;\r\n    if ( localStorage.getItem(\"singleResult\") == undefined || localStorage.getItem(\"singleResult\") == null || localStorage.getItem(\"singleResult\") == \"false\") {\r\n      this.getBetResult();\r\n    } else {\r\n      this.betId = localStorage.getItem(\"betId\");\r\n      this.singleBetResult();\r\n    }\r\n  },\r\n  methods: {\r\n    getBetResult() {\r\n      // 所有賽果列表()\r\n      this.isLoading = true;\r\n      apiGetClubGamblingBetResultList(\"\", true).then(res => {\r\n        if (res.Code == 600) {\r\n          let vm = this;\r\n          const m = vm.$message({\r\n            message: res.Message,\r\n            duration: 3000,\r\n            type: \"error\",\r\n            center: true\r\n          });\r\n          setTimeout(() => {\r\n            m.close();\r\n            this.$router.push(\"/myClub\");\r\n          }, 2000);\r\n          return;\r\n        } else if (res.Data.length == 0) {\r\n          this.isLoading = false;\r\n          this.isNoData = true;\r\n        } else {\r\n          this.isLoading = false;\r\n          this.isNoData = false\r\n          this.resultData = res.Data;\r\n        }\r\n      });\r\n    },\r\n\r\n    // 單一點擊查看\r\n    singleBetResult() {\r\n      let data = {\r\n        clubGamblingId: this.betId\r\n      };\r\n      this.isLoading = true;\r\n      apiGetClubGamblingBetResult(data, true).then(res => {\r\n        if (res.Code == 600) {\r\n          let vm = this;\r\n          const m = vm.$message({\r\n            message: res.Message,\r\n            duration: 3000,\r\n            type: \"error\",\r\n            center: true\r\n          });\r\n          setTimeout(() => {\r\n            m.close();\r\n            this.$router.push(\"/myClub\");\r\n          }, 2000);\r\n          return;\r\n        }else if (res.Data.length == 0) {\r\n          this.isLoading = false;\r\n          this.isNoData = true;\r\n        } else {\r\n          this.isLoading = false;\r\n          this.isNoData = false\r\n          this.resultData = res.Data;\r\n        }\r\n      });\r\n    }\r\n  },\r\n  destroyed() {\r\n    localStorage.setItem(\"singleResult\", false);\r\n  }\r\n};\r\n</script>\r\n"]}]}