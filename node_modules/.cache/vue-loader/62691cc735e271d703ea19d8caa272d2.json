esult() {\r\n      // 所有賽果列表()\r\n      this.isLoading = true;\r\n      apiGetClubGamblingBetResultList(\"\", true).then(res => {\r\n        if (res.Code == 600) {\r\n          let vm = this;\r\n          const m = vm.$message({\r\n            message: res.Message,\r\n            duration: 3000,\r\n            type: \"error\",\r\n            center: true\r\n          });\r\n          setTimeout(() => {\r\n            m.close();\r\n            this.$router.push(\"/myClub\");\r\n          }, 2000);\r\n          return;\r\n        } else if (res.Data.length == 0) {\r\n          this.isLoading = false;\r\n          this.isNoData = true;\r\n        } else {\r\n          this.isLoading = false;\r\n          this.isNoData = false\r\n          this.resultData = res.Data;\r\n        }\r\n      });\r\n    },\r\n\r\n    // 單一點擊查看\r\n    singleBetResult() {\r\n      let data = {\r\n        clubGamblingId: this.betId\r\n      };\r\n      this.isLoading = true;\r\n      apiGetClubGamblingBetResult(data, true).then(res => {\r\n        if (res.Code == 600) {\r\n          let vm = this;\r\n          const m = vm.$message({\r\n            message: res.Message,\r\n            duration: 3000,\r\n            type: \"error\",\r\n            center: true\r\n          });\r\n          setTimeout(() => {\r\n            m.close();\r\n            this.$router.push(\"/myClub\");\r\n          }, 2000);\r\n          return;\r\n        }else if (res.Data.length == 0) {\r\n          this.isLoading = false;\r\n          this.isNoData = true;\r\n        } else {\r\n          this.isLoading = false;\r\n          this.isNoData = false\r\n          this.resultData = res.Data;\r\n        }\r\n      });\r\n    }\r\n  },\r\n  destroyed() {\r\n    localStorage.setItem(\"singleResult\", false);\r\n  }\r\n};\r\n",{"version":3,"sources":["BetResult.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyHA;AACA;AA