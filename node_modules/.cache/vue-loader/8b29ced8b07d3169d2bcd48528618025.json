,\r\n        count: 10\r\n      };\r\n      apiGetClubAnnouncement(data, true).then(res => {\r\n        console.log(res.Data)\r\n        this.clubMsgList = res.Data.Data;\r\n      });\r\n    },\r\n\r\n    // 信息下拉\r\n    isOpen(index) {\r\n      if (this.detailIndex == index && this.isOpenDetail == true) {\r\n        this.isOpenDetail = false;\r\n        return;\r\n      }\r\n      this.detailIndex = index;\r\n      this.isOpenDetail = true;\r\n    },\r\n\r\n    // 開啟發佈頁面\r\n    openRelease() {\r\n      this.isRelease = true;\r\n    }\r\n  },\r\n};\r\n</script>\r\n"]}]}