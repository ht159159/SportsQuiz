t(() => m.close(), 1200);\r\n    }\r\n  }\r\n};\r\n",{"version":3,"sources":["CreateClubRule.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"CreateClubRule.vue","sourceRoot":"src/views/club","sourcesContent":["<template>\r\n  <main class=\"createClubRule\">\r\n    <div class=\"createClubRule-header\" style=\"position:fixed;top: 0\">\r\n      <back class=\"back-icon img-he\"></back>\r\n      <h3>{{ $t(\"CreateClub\") }}</h3>\r\n    </div>\r\n\r\n    <div class=\"createClubRule-level\">\r\n      <div class=\"createClubRule-level-titleBox\">\r\n        <p class=\"createClubRule-level-title\">我的VIP等級 :</p>\r\n        <span>{{ RankId }}</span>\r\n      </div>\r\n      <div class=\"deposit\" @click=\"noGo\">\r\n        {{ $t(\"Deposit\") }}\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"club-Box\">\r\n      <!-- <div class=\"noMissionBg\">\r\n        <img\r\n          class=\"noMission\"\r\n          v-if=\"noMission\"\r\n          src=\"../../assets/暂无.png\"\r\n          alt=\"\"\r\n        />\r\n        <p>{{ $t(\"NoInformation\") }}</p>\r\n      </div> -->\r\n\r\n      <div class=\"createRule\">\r\n        <div class=\"title\">\r\n          <div class=\"title-list\"></div>\r\n          <p>{{ $t(\"創建條件\") }}</p>\r\n        </div>\r\n\r\n        <div class=\"rule\">\r\n          对巴萨来说，梅西是一位至关重要的球员，但是你知道他创造了一个惊人的数据吗？据统计，巴萨在西甲打入的进球中，10%都有梅西的参与！\r\n          西甲创办于1929年，已经将近百年，西甲历史上出过许许多多的球星，而梅西毫无疑问是最重要的球星之一，他的数据也证明了这一点。\r\n          据统计，巴萨俱乐部在西甲总共打入了6167个进球，而梅西打入了其中441球，助攻了其中的182球，也就是说，梅西直接参与了其中623个进球。值得一提的是，有很多的进球，最后的助攻或射门不是来自梅西，但他也参与了的，甚至就是他策划的，但这方面缺少准确的统计数据。\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <button class=\"changeBtn\" @click=\"IsSatisfyMemApplyClub\">{{ $t(\"CreateMyClub\") }}</button>\r\n        <!-- 提示弹窗 -->\r\n    <transition name=\"fade\" :duration=\"{ enter: 500, leave: 300 }\">\r\n      <div class=\"boxBig\" v-show=\"meetConditionsAlert\" style=\"z-index: 1000;\">\r\n        <div class=\"box\">\r\n          <div class=\"head\">\r\n            <div>{{ $t('創建失敗') }}</div>  <!-- 【 創建失敗 】 -->\r\n          </div>\r\n            <div class=\"foot\" style=\"-webkit-user-select:text !important;\">\r\n            <p>{{ isSatisfyMessage }}</p>\r\n            <div class=\"btn-wrap\">\r\n              <div class=\"commit cancelBtn\" @click=\"noGo\">\r\n                {{ $t('Deposit') }}  <!-- 【 充值 】 -->\r\n              </div>\r\n              <div class=\"commit confirmBtn\" @click=\"meetConditionsAlert = false\">\r\n                {{ $t('我知道了') }}  <!-- 【 我知道了 】 -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport { apiIsSatisfyMemApplyClub } from \"@/api/Club.js\";\r\nexport default {\r\n  components: {\r\n    back\r\n  },\r\n  data() {\r\n    return {\r\n      meetConditionsAlert: false,\r\n      // dataMemApplyClub:{\r\n      //   UName: \"\"\r\n      // },\r\n      isSatisfyMessage: \"\",\r\n      RankId: \"\"\r\n    };\r\n  },\r\n\r\n  created() {\r\n    // this.dataMemApplyClub.UName = localStorage.getItem(\"uname\");\r\n    this.RankId = localStorage.getItem(\"RankId\");\r\n  },\r\n\r\n  methods: {\r\n    IsSatisfyMemApplyClub () {\r\n      apiIsSatisfyMemApplyClub(\"\", true).then(res => {\r\n        if(!res.Success){\r\n          this.meetConditionsAlert = true\r\n          this.isSatisfyMessage = res.Message\r\n        }else {\r\n          this.$router.push(\"/CreateClubSet\");\r\n        }\r\n      });\r\n    },\r\n    noGo() {\r\n      const m = this.$message({\r\n        message: this.$t('NotYetOpen'),  //【 暂未开放 】\r\n        type: \"warning\",\r\n        center: true,\r\n        duration: 1200,\r\n        UName: \"no namends\"\r\n      });\r\n      setTimeout(() => m.close(), 1200);\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}