d() {\r\n    localStorage.setItem(\"categoryIndex\", 1);\r\n    this.clubId = localStorage.getItem(\"ClubId\");\r\n    this.getMatchResult();\r\n  },\r\n  methods: {\r\n    getMatchResult() {\r\n      let data = {\r\n        ClubId: this.clubId\r\n      };\r\n      apiGetClubActivityMatchResult(data, true).then(res => {\r\n        if (res.Success.length == 0 || res.Success == false){\r\n          this.noData = true;\r\n        } else {\r\n          this.matchResultObj = res.Success;\r\n        } \r\n      });\r\n    },\r\n    goResultDetail(item) {\r\n      localStorage.setItem(\"AcvtiveId\", item.ClubActivity.Id);\r\n      this.$router.push(\"/ActivityMatchResultDetail\");\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    localStorage.setItem(\"categoryIndex\", 1);\r\n  }\r\n};\r\n</script>\r\n"]}]}