ss-header\" style=\"position: fixed; top: 0\">\r\n        <back class=\"back-icon img-he\"></back>\r\n        <h3>{{ $t(\"ChangePassword\") }}</h3>\r\n      </div>\r\n      <div class=\"changeList\" style=\"margin-top: 1.3rem\">\r\n        <div class=\"inputList\">\r\n          <p class=\"inputList-old\">\r\n            <span class=\"span\">{{ $t(\"OldPassword\") }}：</span>  <!-- 【 OldPassword 】 -->\r\n\r\n            <!-- 【 请输入您的原密码 】 -->\r\n            <el-input\r\n              :type=\"types1\"\r\n              class=\"input\"\r\n              v-model=\"pass1\"\r\n              :placeholder=\"$t('Placeholder_Origin_Password')\"\r\n            ></el-input>\r\n            <img\r\n              @click=\"really1 = !really1\"\r\n              :src=\"\r\n                really1\r\n                  ? require('@/assets/icons/hide1.png')\r\n                  : require('@/assets/icons/hide.png')\r\n              \"\r\n            />\r\n          </p>\r\n          <p class=\"inputList-new\">\r\n            <span>{{ $t(\"NewPassword\") }}：</span>  <!-- 【 新密码 】 -->\r\n\r\n            <!-- 【 请输入您的新密码 】 -->\r\n            <el-input\r\n              :type=\"types2\"\r\n              class=\"input\"\r\n              v-model=\"pass2\"\r\n              :placeholder=\"$t('Placeholder_NewPassword')\"\r\n            ></el-input>\r\n            <img\r\n              @click=\"really2 = !really2\"\r\n              :src=\"\r\n                really2\r\n                  ? require('@/assets/icons/hide1.png')\r\n                  : require('@/assets/icons/hide.png')\r\n              \"\r\n            />\r\n          </p>\r\n          <p class=\"inputList-new\">\r\n            <span>{{ $t(\"ConfirmNewPassword\") }}：</span>  <!-- 【 确认新密码 】 -->\r\n\r\n            <!-- 【 请再次输入您的新密码 】 -->\r\n            <el-input\r\n              :type=\"types\"\r\n              class=\"input\"\r\n              v-model=\"pass3\"\r\n              :placeholder=\"$t('Placeholder_ConfirmNewPassword')\"\r\n            ></el-input>\r\n            <img\r\n              @click=\"really = !really\"\r\n              :src=\"\r\n                really\r\n                  ? require('@/assets/icons/hide1.png')\r\n                  : require('@/assets/icons/hide.png')\r\n              \"\r\n            />\r\n          </p>\r\n        </div>\r\n        <button @click=\"saveUp\" class=\"changeBtn\">{{ $t(\"Save\") }}</button>  <!-- 【 保存 】 -->\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { apiChangePwd } from \"@/api/User.js\";\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport Vue from \"vue\";\r\nimport { Toast } from \"vant\";\r\nVue.use(Toast);\r\nexport default {\r\n  inject: [\"reload\"], //注入reload方法\r\n  components: {\r\n    back\r\n  },\r\n  data() {\r\n    return {\r\n      pass1: \"\",\r\n      pass2: \"\",\r\n      pass3: \"\",\r\n      iconPass: require(\"@/assets/icons/hide1.png\"),\r\n      really: true,\r\n      really2: true,\r\n      really1: true\r\n    };\r\n  },\r\n  computed: {\r\n    types() {\r\n      return this.really ? \"password\" : \"text\";\r\n    },\r\n    types2() {\r\n      return this.really2 ? \"password\" : \"text\";\r\n    },\r\n    types1() {\r\n      return this.really1 ? \"password\" : \"text\";\r\n    }\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    },\r\n    saveUp() {\r\n      let data = {\r\n        OldPwd: this.pass1,\r\n        NewPwd: this.pass2,\r\n        ConfirmNewPwd: this.pass3\r\n      };\r\n      if (data.OldPwd == \"\" || data.NewPwd == \"\" || data.ConfirmNewPwd == \"\") {\r\n        // return this.$Alert.showWarning.call(this, this.$t(\"PasswordNotFilled\"))\r\n        return this.Alert.showMsg(\r\n          this.$t(\"PasswordNotFilled\"),\r\n          \"warning\",\r\n          1200,\r\n          true,\r\n          true\r\n        );\r\n      }\r\n      apiChangePwd(data, true).then(res => {\r\n        if (res.IsSuccess) {\r\n          localStorage.removeItem(\"token\");\r\n          const m = this.$message({\r\n            message: res.message,\r\n            type: \"success\",\r\n            center: true,\r\n            duration: 1000\r\n          });\r\n          setTimeout(() => m.close(), 1200);\r\n          setTimeout(() => {\r\n            this.$router.push(\"/\");\r\n          }, 1500);\r\n        } else {\r\n          const m = this.$message({\r\n            message: res.message,\r\n            type: \"error\",\r\n            center: true,\r\n            duration: 1000\r\n          });\r\n          setTimeout(() => m.close(), 1200);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.el-input {\r\n  width: 4rem;\r\n  border: 0;\r\n  /* height: 34px; */\r\n  margin-right: 0.1rem;\r\n  margin-left: 0.5rem;\r\n}\r\n\r\n.input .el-input__inner {\r\n  display: inline-block;\r\n  width: 4rem;\r\n  font-size: 0.24rem;\r\n  border: 0;\r\n  padding-left: 0.1rem;\r\n  height: 0.8rem;\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"]}]}