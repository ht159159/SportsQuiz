div class=\"ranking\" v-else>\r\n          <img class=\"trophy\" src=\"@/assets/rankboard/third.png\" />\r\n          <img src=\"../assets/排行榜_08.jpg\" alt=\"\" class=\"rankingimg\" />\r\n          <div class=\"ranking-main\">\r\n            <p class=\"rankname\">{{ $t('WaitAndSee') }}</p>\r\n            <p class=\"yellow-text\">{{ unit }} : 0</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 其他名次 -->\r\n      <div class=\"other-rankins\">\r\n        <div class=\"rank-item\">\r\n          <p class=\"purple-text\">{{ $t('Rank') }}</p>\r\n          <p class=\"purple-text\">{{ $t('Nickname') }}</p>\r\n          <p class=\"purple-text\">{{ unit }}</p>\r\n          <p class=\"purple-text\">{{ $t('Detailed') }}</p>\r\n        </div>\r\n\r\n        <!-- 第四名以下資料 -->\r\n        <ul  class=\"other-rankins-wrap\"  v-if=\"list.length > 0\"  v-infinite-scroll=\"load\"  infinite-scroll-disabled=\"busy\">\r\n          <li v-for=\"(item, index) in list\">\r\n            <div>TOP {{ index + 4 }}</div>\r\n            <div class=\"other-rankings-NickName\">\r\n              <img :src=\"second.UImg\" />\r\n              <p>{{ item.NickName }}</p>\r\n            </div>\r\n            <div :class=\"[index + 4 <= 10 ? 'orange-text' : 'gold-text']\">\r\n              {{ item.Amount }}\r\n            </div>\r\n            <div class=\"detail-wrap\">\r\n              <span class=\"detail-check\" @click=\"notOpen()\">{{ $t('TakePeek') }}</span>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n        <!-- 無第四名以下資料時 -->\r\n        <div class=\"noDataRank\" v-else></div>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n<script>\r\nimport back from \"@/components/Botton/back.vue\";\r\nimport {\r\n  apiGetBetAmount,\r\n  apiGetWinCount,\r\n  apiGetWinAmount\r\n} from \"@/api/Rankboard\";\r\nexport default {\r\n  components: {\r\n    back\r\n  },\r\n  data() {\r\n    return {\r\n      rankType: undefined,\r\n      rankTypeName: \"\",\r\n      unit: