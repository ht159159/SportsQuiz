.recommended .content::-webkit-scrollbar {
    display: none;
}
.clubAdPutIn {
    width: 100%;
    height: 100%;
    background: #f2f2f2;
    position: relative;
    &-header {
        width: 100%;
        height: 1.1rem;
        line-height: 1.2rem;
        text-align: center;
        @include page_header_color($theme-red);
        z-index: 1;

        h3 {
            @include page_header_text_color($theme-red-header-text-color);
            display: inline-block;
            margin-left: -0.5rem;
        }
        .img-he {
            margin-left: 0.3rem;
            float: left;
            margin-top: 0.4rem;
            width: 0.42rem;
            height: 0.42rem;
        }
        .img-he:active {
            transform: scale(0.95);
        }
    }
    .club-Box {
        width: 100%;
        overflow-y: scroll;
        margin-top: 1.1rem;
        // text-align: center;
        .createSet {
            width: 100%;
            padding: 0.22rem;
            box-sizing: border-box;

            .setField {
                margin-bottom: 0.3rem;

                .title {
                    height: 0.4rem;
                    display: flex;
                    justify-content: flex-start;
                    // align-items: center;
                    color: #000;
                    // margin-bottom: 0.2rem;

                    &-list {
                        width: 0.1rem;
                        height: 65%;
                        background-color: #6774e6;
                        border-radius: 0.1rem;
                        margin: 0 0.1rem;
                    }

                    p {
                        height: 100%;
                        display: flex;
                        font-size: 0.28rem;
                    }

                    .required {
                        color: red;
                        display: flex;
                        align-items: center;
                        font-size: 0.36rem;
                    }

                    .multipleChoice {
                        font-size: 0.28rem;
                        color: #7a7a7a;
                    }
                }

                .chooseRace {
                    width: 48%;
                    height: 0.86rem;
                    border-radius: 0;
                    filter: drop-shadow(0 0 0.08rem rgba(73, 75, 87, 0));
                    background-color: #fff;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    position: relative;
                    flex-direction: row-reverse;

                    .race {
                        width: 0.7rem;
                    }
                }
                .uploadRulesBox {
                    padding-left: 0.26rem;
                    margin-top: 0.2rem;
                    color: #646464;
                    .uploadRules {
                        display: flex;
                        margin-bottom: 0.1rem;
                        .uploadRulesNumber {
                            width: 0.24rem;
                            height: 0.24rem;
                            border-radius: 0.36rem;
                            background-color: #909090;
                            color: #fff;
                            text-align: center;
                            font-size: 0.24rem;
                        }
                        .uploadRulesContent{
                            width: 90%;
                            line-height: 0.3rem;
                        }
                    }
                }
                .uploadImg {
                    width: 100%;
                    height: 3.1rem;
                    // background-color: #fff;
                    border: 2px dotted #6774e6;
                    margin: 0 auto;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    flex-flow: column;
                    position: relative;

                    .uploadIcon {
                        width: 0.6rem;
                        height: 0.6rem;
                        background: url("~@/assets/club/upload-icon.png");
                    }
                    .uploadContent {
                        color: #646464;
                        margin-top: 0.2rem;
                    }

                    .imgInput {
                        position: absolute;
                        top: 0;
                        left: 0;
                        width: 100%;
                        height: 100%;
                        opacity: 0;
                    }
                    #demo {
                        height: 100%;
                        position: absolute;
                    }
                }
                .changeAdTitle{
                    display: flex;
                    justify-content: center;
                    color: #646464;
                    margin: 0.2rem 0;
                }
            }
        }
    }

    .noMissionBg {
        text-align: center;
        position: relative;
        p {
            position: absolute;
            bottom: 0.2rem;
            left: 50%;
            transform: translateX(-50%);
            color: #8b8b8b;
        }
    }
    .noMission {
        width: 65%;
        margin-top: 1rem;
    }
    .btnBox {
        width: 100%;
        position: absolute;
        bottom: 0;
    }
    .changeBtn {
        display: block;
        border: 0;
        width: 6.82rem;
        height: 0.8rem;
        border-radius: 0.6rem;
        @include myself_btn();
        margin: 0 auto;
        outline: none;
        box-shadow: 3px 6px 9px #dfa9ae;
        margin-bottom: 0.5rem;
        transition: 0.3s;
    }
    .changeBtn:active {
        background-color: #c9c9c9;
        transform: scale(0.95);
    }
    .depositBtn {
        @include adRecharge_btn();
    }

    .el-checkbox-group { 
        width: 100%;
        display: flex;
        justify-content: space-between;
    }
    .el-checkbox{
        margin: 0;
    }
    .el-checkbox__label{
        padding: 0;
        padding-right: 0.2rem;
        color: #7e7e7e;
    }
    .el-checkbox__input{
        display: flex;
    }
    .el-checkbox__input.is-checked .el-checkbox__inner,
    .el-checkbox__input.is-indeterminate .el-checkbox__inner {
        background-color: #5e6dee;
        border-color: #5e6dee;
    }
    .el-checkbox__input.is-checked + .el-checkbox__label {
        color: #5e6dee;
    }
    .el-checkbox.is-bordered.is-checked {
        border-color: #5e6dee;
        .el-checkbox__label{
            color: #000;
        }
    }
    .el-checkbox__input.is-focus .el-checkbox__inner {
        border-color: #5e6dee;
    }
    .el-checkbox__inner:hover{
        border-color: #5e6dee;
    }
    .el-checkbox.is-bordered.is-checked {
        // border-color: #5e6dee;
        border: 2px solid #5e6dee;
    }
}
