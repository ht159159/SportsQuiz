.recommended .content::-webkit-scrollbar {
    display: none;
}
.createClubSet {
    width: 100%;
    height: 100%;
    background: #f2f2f2;
    position: relative;
    &-header {
        width: 100%;
        height: 1.1rem;
        line-height: 1.2rem;
        text-align: center;
        @include page_header_color($theme-red);
        z-index: 1;

        h3 {
            @include page_header_text_color($theme-red-header-text-color);
            display: inline-block;
            margin-left: -0.5rem;
        }
        .img-he {
            margin-left: 0.3rem;
            float: left;
            margin-top: 0.4rem;
            width: 0.42rem;
            height: 0.42rem;
        }
        .img-he:active {
            transform: scale(0.95);
        }
    }
    .createClubRule-level {
        width: 100%;
        // margin-top: 1.1rem;
        position: fixed;
        top: 1.1rem;
        display: flex;
        justify-content: space-between;
        align-items: center;
        background-color: #fff;
        z-index: 1;
        filter: drop-shadow(0px 0.1rem 0.13rem rgba(147, 147, 147, 0.29));

        .createClubRule-level-titleBox {
            width: auto;
            height: 0.8rem;
            display: flex;
            align-items: center;
            margin-left: 0.3rem;

            p {
                width: auto;
                height: 100%;
                text-align: center;
                font-size: 0.28rem;
                line-height: 0.8rem;
                color: #232323;
                margin-right: 0.2rem;
            }
        }

        .deposit {
            width: 0.94rem;
            height: 0.36rem;
            display: flex;
            justify-content: center;
            align-items: center;
            border-radius: 0.18rem;
            background-image: linear-gradient(0deg, #697bff 41%, #918fff 100%);
            font-size: 0.24rem;
            color: #fff;
            margin-right: 0.3rem;
        }
    }

    .club-Box {
        width: 100%;
        overflow-y: scroll;
        margin-top: 1.1rem;
        // text-align: center;

        .recommended {
            width: 100%;
            // height: 2.6rem;
            // background: -webkit-linear-gradient(left, #576ede, #7779ee);
            // background: -o-linear-gradient(right, #576ede, #7779ee);
            // background: -moz-linear-gradient(right, #576ede, #7779ee);
            // background: linear-gradient(to right, #576ede, #7779ee);
            background: url("~@/assets/club/recommended-bg.png");
            background-size: 100% 100%;
            padding: 0.22rem 0;
            box-sizing: border-box;
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            justify-content: space-between;
            overflow: auto;
            margin-top: 0.2rem;
            .title {
                height: 0.4rem;
                display: flex;
                justify-content: flex-start;
                align-items: center;
                color: #fff;
                margin-bottom: 0.2rem;
                margin-left: 0.2rem;

                .title_list {
                    width: 0.1rem;
                    height: 65%;
                    background-color: #fff;
                    border-radius: 0.1rem;
                    margin: 0 0.1rem 0 0;
                }
                p {
                    height: 100%;
                    line-height: 0.4rem;
                    font-size: 0.3rem;
                }
            }
            .content {
                width: 100%;
                overflow-x: auto;
                overflow-y: hidden;
                display: flex;
                position: relative;

                .recommended-club {
                    width: 3.34rem;
                    height: 1.54rem;
                    margin: 0 0.09rem;
                    padding: 0.05rem;
                    box-sizing: border-box;
                    position: relative;
                    background-color: #6774e6;
                    filter: drop-shadow(0px 3px 3.5px rgba(36, 49, 159, 0.67));
                    flex: none;
                    &-img {
                        width: 100%;
                        height: 100%;
                    }
                }
                .recommended-club:first-child {
                    margin-left: 0.36rem;
                }
            }
        }
        .createSet {
            width: 100%;
            padding: 0.22rem;
            box-sizing: border-box;

            .setField {
                width: 100%;
                margin-bottom: 0.3rem;

                .title {
                    height: 0.4rem;
                    display: flex;
                    justify-content: flex-start;
                    // align-items: center;
                    color: #000;
                    // margin-bottom: 0.2rem;

                    &-list {
                        width: 0.1rem;
                        height: 65%;
                        background-color: #6774e6;
                        border-radius: 0.1rem;
                        margin: 0 0.1rem;
                    }

                    p {
                        height: 100%;
                        display: flex;
                        font-size: 0.28rem;
                    }

                    .required {
                        color: red;
                        display: flex;
                        align-items: center;
                        font-size: 0.36rem;
                    }

                    .multipleChoice {
                        font-size: 0.28rem;
                        color: #7a7a7a;
                    }
                }
                .setInformation {
                    width: 100%;
                    height: 0.7rem;
                    border-radius: 5px;
                    background-color: #fff;
                    border: 1px solid #cdcdcd;
                }

                .chooseRace {
                    width: 1.5rem;
                    height: 1.46rem;
                    border-radius: 5px;
                    filter: drop-shadow(0 0 0.08rem rgba(73, 75, 87, 0));
                    background-color: #fff;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    position: relative;

                    .race {
                        width: 0.7rem;
                    }
                }
                .noMore, .push{
                    z-index: 1;
                    width: 100%;
                    margin: 0.1rem 0;
                    color: #7a7a7a;
                }
                .uploadRulesBox {
                    padding-left: 0.26rem;
                    .uploadRules {
                        display: flex;
                        margin-bottom: 0.1rem;
                        .uploadRulesNumber {
                            width: 0.24rem;
                            height: 0.24rem;
                            border-radius: 0.36rem;
                            background-color: #5c5c5c;
                            color: #fff;
                            text-align: center;
                            font-size: 0.24rem;
                        }
                        .uploadRulesContent{
                            width: 90%;
                            line-height: 0.3rem;
                        }
                    }
                }
                .uploadImg {
                    width: 4.76rem;
                    height: 2.34rem;
                    background-color: #fff;
                    border: 2px dotted #cdcdcd;
                    margin: 0 auto;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    flex-flow: column;
                    position: relative;

                    .uploadIcon {
                        width: 0.6rem;
                        height: 0.6rem;
                        background: url("~@/assets/club/upload-icon.png") no-repeat;
                        background-size: 100% 100%;
                    }
                    .uploadContent {
                        color: #646464;
                        margin-top: 0.2rem;
                    }

                    .imgInput {
                        position: absolute;
                        top: 0;
                        left: 0;
                        width: 100%;
                        height: 100%;
                        opacity: 0;
                    }
                    #demo {
                        height: 100%;
                        position: absolute;
                    }
                }
            }
        }
    }

    .noMissionBg {
        text-align: center;
        position: relative;
        p {
            position: absolute;
            bottom: 0.2rem;
            left: 50%;
            transform: translateX(-50%);
            color: #8b8b8b;
        }
    }
    .noMore, .push{
        z-index: 1;
        width: 100%;
        // margin: 0.1rem 0;
        color: #7a7a7a;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
    }
    .noMission {
        width: 65%;
        margin-top: 1rem;
    }
    .fade-enter-active,
    .fade-leave-active {
        transition: opacity 0.5s;
    }

    .fade-enter,
    .fade-leave-to {
        opacity: 0;
    }

    .fade-leave,
    .fade-enter-to {
        opacity: 1;
    }

    .changeBtn {
        color: #fff;
        background: -webkit-gradient(linear, left top, right top, from(#e73a51), to(#dc3849));
        background: linear-gradient(to right, #e73a51, #dc3849);
        border: none;
    }
    .changeBtn:active {
        background-color: #c9c9c9;
        transform: scale(0.95);
    }
    .changeBtn {
        display: block;
        border: 0;
        width: 6.82rem;
        height: 0.8rem;
        border-radius: 0.6rem;
        @include myself_btn();
        margin: 0 auto;
        outline: none;
        box-shadow: 3px 6px 9px #dfa9ae;
        transition: 0.3s;
    }
    .changeBtn:active {
        background-color: #c9c9c9;
        transform: scale(0.95);
    }
    .el-checkbox-group {
        width: 100%;
        height: 3.6rem;
        display: flex;
        position: relative;    
        // flex-flow: wrap column;
        flex-direction: column;
        flex-wrap: wrap;
        align-content: flex-start;
        // overflow-x: scroll;
        overflow-y: hidden;
        overflow-x: scroll;
        -webkit-overflow-scrolling: touch;
    }
    .el-checkbox {
        margin-right: 0.26rem;
        margin-top: 0.3rem;
    }
    .el-checkbox__input {
        position: absolute;
        top: 0.06rem;
        right: 0.06rem;
    }
    .el-checkbox__inner {
        border-radius: 10px;
    }
    .el-checkbox__input.is-checked .el-checkbox__inner,
    .el-checkbox__input.is-indeterminate .el-checkbox__inner {
        background-color: #01d501;
        border-color: #f2f2f2;
        border-radius: 10px;
    }
    .el-checkbox__input.is-checked + .el-checkbox__label {
        color: #01d501;
    }
    // .el-checkbox.is-bordered.is-checked {
    //     border-color: #f2f2f2;
    // }
    .el-checkbox__input.is-focus .el-checkbox__inner {
        border-color: #f2f2f2;
    }
    .el-checkbox__label {
        padding: 0;
        display: flex;
        flex-flow: column;
        justify-content: center;
        align-items: center;

        p{
            margin-top: 0.1rem;
            white-space: pre-wrap;
            text-align: center;
        }
    }
    .el-checkbox__inner::after {
        transform: none;
        border: 1px solid #d1d1d1;
        border-left: 0;
        border-top: 0;
        transform: rotate(45deg) scaleY(1);
    }
    .el-checkbox__input.is-checked .el-checkbox__inner::after {
        border-color: #fff;
    }

    .swiper-container {
        height: 4.5rem;
        z-index: 0;
    }
    .swiper-slide {
        padding: 0 0 0.5rem 0;
    }
    .swiper-slide {
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-items: flex-start;
    }
}
