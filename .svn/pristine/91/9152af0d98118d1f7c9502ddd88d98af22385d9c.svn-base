* {
  font-family: '微软雅黑';
}
.rank {
  height: 100%;
  position: relative;
  .explain {
    z-index: 9999;
    position: absolute;
    left: 50%;
    top: 3rem;
    transform: translateX(-50%);
    background-image: url('~@/assets/rank-explain.png');
    background-size: 100%;
    // border-radius: 0.1rem /* 8/100 */;
    box-sizing: border-box;
    width: 6.46rem /* 646/100 */;
    height: 7.72rem /* 772/100 */;
    // background-color: #1c202e;
    // border: 2px solid #9196a6;
    color: #f2f2f2;

    padding-top: 0.58rem /* 58/100 */;
    padding-left: 0.47rem /* 47/100 */;
    padding-right: 0.47rem;
    .rank-popup-title {
      text-align: center;
      font-size: 0.34rem; /* 34/100 */
      margin: 0 auto;
      margin-bottom: 0.48rem; /* 48/100 */
    }
    .rank-popup-content {
      line-height: 0.55rem; /* 55/100 */
      color: #989ca8;
      font-size: 0.22rem; /* 18/100 */
    }
    .explain-x {
      position: absolute;
      top: 0.18rem /* 18/100 */;
      right: 0.19rem /* 19/100 */;
      display: inline-block;
      width: 0.36rem /* 36/100 */;
      height: 0.36rem /* 36/100 */;
      background-image: url('~@/assets/icons/x.png');
      background-size: 100%;
    }
  }
  &-head {
    height: 1.1rem /* 124/100 */;
    @include page_header_color($theme-red);
    text-align: center;
    line-height: 1.1rem;
    @include page_header_text_color($theme-red-header-text-color);
    font-size: 0.33rem; /* 33/100 */
    position: fixed;
    top: 0;
    width: 100%;
  }
  &-main {
    margin-top: 1.1rem;
    height: calc(100% - 2.23rem);
    position: relative;
    // 头部
    &-top {
      position: relative;
      height: 5.22rem /* 413/100 */;
      background-repeat: no-repeat;
      background-size: 100%;
      .van-button {
        position: absolute;
        line-height: 0.67rem;
        width: 4.32rem /* 432/100 */;
        height: 0.81rem /* 81/100 */;
        background-repeat: no-repeat;
        background-size: 100%;
        border: none;
        left: 50%;
        top: 2.32rem;
        transform: translateX(-50%);
        // margin: 0 auto;
        // margin-top: 2.32rem /* 232/100 */;
      }
      .van-button-CN {
        background-image: url('~@/assets/rank-button4_CN.png');
      }
      .van-button-TW {
        background-image: url('~@/assets/rank-button4_TW.png');
      }
      .van-button-US {
        background-image: url('~@/assets/rank-button4_US.png');
      }
      .van-button--info {
        background-color: none;
      }
      .question {
        display: inline-block;
        width: 0.31rem /* 31/100 */;
        height: 0.31rem;
        font-size: 0.5rem; /* 50/100 */
        float: right;
        margin-top: 0.2rem /* 20/100 */;
        margin-right: 0.19rem; /* 19/100 */
        background-image: url('~@/assets/questyellow.png');
        background-repeat: no-repeat;
        background-size: 100%;
      }
    }
    &-top-CN {
      background-image: url('~@/assets/head_01_CN.png');
    }
    &-top-TW {
      background-image: url('~@/assets/head_01_TW.png');
    }
    &-top-US {
      background-image: url('~@/assets/head_01_US.png');
    }
    &-tabs {
      margin: 0 auto;
      // height: 1.06rem /* 106/100 */;
      // max-width: 7.5rem /* 750/100 */;
      width: 100%;
      position: absolute;
      top: -1.2rem;
      .myrank {
        float: left;
        margin-left: 1.31rem;
        margin-top: 0.3rem;
      }
      .tab-ranklist {
        float: right;
        margin-right: 1.31rem;
        margin-top: 0.3rem;
      }
      .hide1 {
        height: 0.8rem /* 123/100 */;
        width: 50%;
        float: left;
        position: absolute;

        left: 0;
        top: -0.6rem;
        z-index: 5;
      }
      .hide2 {
        height: 0.8rem /* 123/100 */;
        width: 50%;
        float: right;
        position: absolute;
        // background-color: green;
        right: 0;
        top: -0.6rem;
        z-index: 5;
      }
      position: relative;
      .test1 {
        width: 100%;
        height: 1.22rem /* 122/100 */;
        background-image: url('~@/assets/rank1.png');
        background-size: 100%;
        position: absolute;
        z-index: 1;
        left: 0;
        top: -0.6rem;
        color: #c02f3e;
        font-size: 0.24rem; /* 24/100 */
        font-weight: bold;
        background-repeat: no-repeat;
      }
      .test2 {
        width: 100%;
        height: 1.03rem;
        background-image: url('~@/assets/rank2.png');
        background-size: 100%;
        background-repeat: no-repeat;
        position: absolute;
        right: 0;
        top: -0.59rem;
        color: #848484;
        font-size: 0.24rem; /* 24/100 */
        font-weight: bold;
        // z-index: 2;
      }

      .test-active {
        z-index: 3;
      }
      .test1-bgc {
        background-image: url('~@/assets/rank3.png');
        background-size: 100%;
        background-repeat: no-repeat;
        color: #848484;
      }
      .test2-bgc {
        background-image: url('~@/assets/rank4.png');
        background-size: 100%;
        background-repeat: no-repeat;
        color: #c02f3e;
        z-index: 4;
      }
      .van-tab.van-tab--active {
        z-index: 1;
      }

      .van-tab1 {
        background-color: red;
      }
      .rank-tabs-cell {
        box-sizing: border-box;
        margin: 0 auto;
        background-color: #e2e2e2;
        width: 7.1rem /* 710/100 */;
        height: 0.9rem /* 90/100 */;
        border-radius: 3px;
        line-height: 0.9rem;
        padding-left: 0.25rem /* 25/100 */;
        padding-right: 0.27rem;
        margin-bottom: 0.34rem;
        .rank-cell-span {
          font-size: 0.24rem /* 24/100 */;
          color: #494949;
        }
      }
      .rank-tabs-cell:first-child {
        margin-top: 0.34rem; /* 34/100 */
      }
      .van-tabs__line {
        display: none;
      }
    }

    .rank-myrank-active {
      background-image: url('~@/assets/rank-background_03.png');
      background-size: 70%;
      background-repeat: no-repeat;
      background-position: center 1rem /* 246/100 */;
    }
    .rank-myrank {
      width: 100%;
      height: calc(100% - 4.6rem);
      position: relative;
      overflow: auto;
      margin-top: -0.55rem;
      -webkit-overflow-scrolling: touch;

      // z-index: 99;
    }

    .wipeLoading{
      p{
        text-align: center;
      }
    }

    .myrank-list {
      width: calc(100% - 0.4rem);
      height: 0.91rem /* 91/100 */;
      margin-bottom: 0.34rem /* 34/100 */;
      margin-left: 0.2rem; /* 20/100 */
      margin-right: 0.2rem;
      background-color: #e2e2e2;
      border-radius: 0.05rem; /* 5/100 */
      line-height: 0.91rem;
      display: flex;
      justify-content: space-between;
      align-items: center;
      &-img {
        width: 0.5rem;
        height: 0.5rem;
        margin-left: 0.27rem;
      }
      span {
        color: #474747;
        font-size: 0.24rem /* 24/100 */;
        margin-left: 0.26rem /* 26/100 */;
        font-weight: 500;
        margin-right: auto;
      }
      .rank-arrow {
        width: 0.28rem /* 28/100 */;
        height: 0.28rem /* 28/100 */;
        background-image: url('~@/assets/icons/arrow.png');
        background-size: 100%;
        float: right;
        margin-right: 0.27rem /* 27/100 */;
        // margin-top: 0.32rem /* 32/100 */;
      }
    }
    .rank-match-list {
      // width: calc(100% - 0.4rem);
      position: relative;
      width: 100%;
      height: calc(100% - 4.6rem);
      overflow: auto;
      -webkit-overflow-scrolling: touch;
      margin-top: -0.55rem;
      z-index: 0;
      .match-list-football {
        width: calc(100% - 0.4rem);
        height: 0.91rem /* 91/100 */;
        margin-bottom: 0.34rem /* 34/100 */;
        margin-left: 0.2rem; /* 20/100 */
        margin-right: 0.2rem;
        background-color: #e2e2e2;
        border-radius: 0.05rem; /* 5/100 */
        line-height: 0.91rem;
        display: flex;
        justify-content: space-between;
        align-items: center;
        &-img {
          width: 0.5rem;
          height: 0.5rem;
          margin-left: 0.27rem;
        }
        span {
          color: #474747;
          font-size: 0.24rem /* 24/100 */;
          margin-left: 0.26rem /* 26/100 */;
          font-weight: 500;
          margin-right: auto;
        }
        .rank-arrow {
          display: inline-block;
          width: 0.28rem /* 28/100 */;
          height: 0.28rem /* 28/100 */;
          background-image: url('~@/assets/icons/arrow.png');

          background-size: 100%;
          float: right;
          margin-right: 0.27rem /* 27/100 */;
          // margin-top: 0.32rem /* 32/100 */;
        }
        transition: 0.3s;
      }
      .match-list-football:active {
        background-color: #c9c9c9;
        transform: scale(0.95);
      }
    }
  }
  .van-button--info {
    background-color: none;
  }
}
