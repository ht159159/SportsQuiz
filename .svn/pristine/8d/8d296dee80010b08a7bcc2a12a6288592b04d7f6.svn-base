[v-cloak] {
  display: none;
}
.gameRes {
  background-color: #fff;
  .nomatchs {
    // width: 5.01rem /* 501/100 */;
    height: 5.42rem /* 542/100 */;
    // background-color: red;
    margin-top: 0.3rem /* 30/100 */;
    background-image: url('~@/assets/noInfo.png');
    background-position: center;
    background-size: 65%;
    background-repeat: no-repeat;
    position: relative;
    p {
      position: absolute;
      bottom: 0.7rem;
      left: 50%;
      transform: translateX(-50%);
      color: #8b8b8b;
    }
  }
  .contentUl {
    li {
      flex: auto;
    }
  }
  width: 100%;
  height: 100%;
  // background: #fff;
  &-header {
    width: 100%;
    height: 1.1rem;
    line-height: 1.2rem;
    text-align: center;
    @include page_header_color($theme-red);
    z-index: 999;
    top: 0;
    h3 {
      @include page_header_text_color($theme-red-header-text-color);
      display: inline-block;
      // margin-left: -1rem;
      position: absolute;
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%);
    }
    .img-he {
      margin-left: 0.5rem;
      float: left;
      margin-top: 0.3rem;
      width: 0.42rem;
      height: 0.42rem;
    }
    position: fixed;
    .back-icon {
      position: absolute;
      left: 0.2rem /* 20/100 */;
    }
  }
  &-content {
    margin-top: 1.06rem;
    width: 100%;
    // background: #f2f2f2;
    &-time {
      display: flex;
      height: 0.78rem;
    }
    &-ul {
      display: flex;
      justify-content: space-around;
      padding: 0;
      width: 100%;
      height: 1.06rem;
      line-height: 1.06rem;
      background: #fff;
      list-style: none;
      margin: 0;
      text-align: center;

      &-item {
        font-size: 0.28rem;
        text-align: center;
        color: #5d5d5d;
        width: 1rem;
        // flex: 1.5,1.5,2,1.5,1.5,1.5;
      }
    }
  }
  .active {
    color: #c12e3e;
    // border-bottom: .07rem solid #c12e3e;
    // height: .99rem;
    // width: .95rem;
  }
  .soprtList {
    width: 100%;
    height: 11.45rem;
    background: #f2f2f2;
    padding-top: 0.08rem;
    overflow: auto;
    height: calc(100% - 2.1rem);
    -webkit-overflow-scrolling: touch;
    // margin-top: -.1rem;
    &-ul {
      list-style: none;
      width: 100%;
      padding: 0;
      &-item {
        width: calc(100% - 0.22rem);
        margin: 0 auto;
        margin-bottom: 0.1rem /* 10/100 */;
        &-p {
          font-size: 0.16rem;
          line-height: 0.37rem;
          color: #707070;
          text-align: center;
          &-img {
            display: inline-block;
            vertical-align: middle;
          }
        }
        &-div {
          background: #fff;
          width: 100%;
          height: 2.16rem;
          padding-top: 0.08rem;
          border-radius: 0.2rem;
          box-shadow: 1px 3px 5px #eae9e9;
          margin-bottom: 0.05rem /* 5/100 */;
          .sP {
            text-align: center;
            font-size: 0.24rem;
            color: #3c3c3c;
            line-height: 0.48rem;
          }
          .sContent {
            width: 6.9rem;
            margin: 0 auto;
            position: relative;
            display: flex;
            .imgVs {
              position: absolute;
              left: 3rem;
              top: 0;
            }
            .hong {
              display: inline-block;
              width: 3.42rem;
              height: 0.48rem;
              box-shadow: 1px 3px 5px #fad9db;
              font-size: 0.2rem;
              color: #fff;
              text-align: center;
              border-radius: 0.4rem;
              line-height: 0.48rem;
              background: linear-gradient(to right, #ba1825, #e22231);
              margin-top: 0.14rem;
              box-shadow: 6px 6px 6px #8c7d7f;
              .hong-span {
                font-size: 0.19rem /* 19/100 */;
              }
            }
            .lan {
              display: inline-block;
              width: 3.42rem;
              height: 0.48rem;
              box-shadow: 1px 3px 5px #d7deeb;
              font-size: 0.2rem;
              color: #fff;
              text-align: center;
              border-radius: 0.4rem;
              line-height: 0.48rem;
              background: linear-gradient(to right, #163f8c, #0d2f6f);
              margin-top: 0.14rem;
              box-shadow: 6px 6px 6px #8c7d7f;
              .lan-span {
                font-size: 0.19rem /* 19/100 */;
              }
            }
          }
          .gameFoot {
            width: 100%;
            .gameH {
              margin-left: -0.2rem /* 1/100 */;
            }
            &-left {
              float: left;
              margin-left: 0.93rem;
              margin-top: 0.28rem;
              color: #5f5f5f;
              font-size: 0.2rem;
              font-weight: 700;
              p {
                text-align: center;
                // width: max-content;
              }
              .gameRight {
                margin-left: 0.37rem;
              }
              .gameRightB {
                line-height: 0.4rem;
              }
              .gameRightA {
                line-height: 0.4rem;
              }
            }
            &-right {
              float: right;
              margin-right: 1.3rem;
              margin-top: 0.28rem;
              color: #5f5f5f;
              font-size: 0.2rem;
              font-weight: 700;
              p {
                text-align: center;
                width: max-content;
              }
              .gameRight {
                margin-left: 0.37rem;
              }
              .gameRightB {
                line-height: 0.4rem;
              }
              .gameRightA {
                line-height: 0.4rem;
              }
            }
          }
        }
      }
    }
  }
  .left .el-input__inner,
  .right .el-input__inner {
    line-height: 0.56rem;
  }
  .el-date-editor .el-icon-circle-close {
    display: none;
  }
}
