.back-icon {
  width: 0.42rem /* 42/100 */;
  height: 0.42rem /* 42/100 */;
  display: inline-block;
  // background-image: url('~@/assets/back.png');
  // background-size: 50%;
  @include back_icon();
  background-repeat: no-repeat;
  margin-left: 0.31rem /* 31/100 */;
  margin-top: 0.5rem;
  transition: 0.3s;
}
.back-icon:active {
  transform: scale(0.95);
}
