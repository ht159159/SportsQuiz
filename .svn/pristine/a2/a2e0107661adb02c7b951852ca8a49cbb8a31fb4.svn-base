.self {
    width: 100%;
    height: 100%;
    background: #f2f2f2;
    position: relative;
    &-header {
        width: 100%;
        height: 1.1rem;
        line-height: 1.2rem;
        text-align: center;
        @include page_header_color($theme-red);
        h3 {
            @include page_header_text_color($theme-red-header-text-color);
            display: inline-block;
            margin-left: -0.6rem;
        }
        .img-he {
            margin-left: .3rem;
            float: left;
            margin-top: .4rem;
            width: .42rem;
            height: .42rem;
        }
        .img-he:active {
            transform: scale(.95);
        }
    }
    &-ul {
        padding-top: .32rem;
        padding-left: 0;
        -webkit-user-select: text !important;
        &-item {
            height: .86rem;
            width: calc(100% - .34rem);
            margin: 0 auto;
            border-bottom: 1px solid #cecccc;
            line-height: .86rem;
            display: flex;
            justify-content: flex-start;
            align-items: center;
            .bigp {
                width: 1.6rem;
                height: 1.6rem;
                border-radius: 50%;
                margin: 0 auto;
                margin-top: .3rem;
                img {
                    display: inline-block;
                    width: 100%;
                    height: 100%;
                    border-radius: 1rem;
                }
            }
            .imgList {
                display: flex;
                justify-content: space-between;
                flex-wrap: wrap;
                padding: 0 .3rem;
                li {
                    // width: .96rem;
                    width: 20%;
                    height: .96rem;
                    border-radius: 50%;
                    margin-top: .16rem;
                    img {
                        display: inline-block;
                        width: 100%;
                        height: 100%;
                    }
                }
            }
            .righDiv {
                display: inline-block;
                margin-left: .5rem;
                p {
                    display: inline-block;
                    width: 1.75rem;
                    height: .5rem;
                    // background: #aaaaaa;
                    border-radius: 1rem;
                    text-align: right;
                    line-height: .5rem;
                    font-size: .22rem;
                    color: #fff;
                    padding-right: .2rem;
                }
            }
            &-span {
                width: 15%;
                color: #202020;
                font-size: .24rem;
            }
            &-input {
                border: 0;
                height: .28rem;
                font-size: .16rem;
                color: #acacac;
                outline: none;
            }
            // input::-webkit-input-placeholder{
            //     font-size: .15rem;
            //     }
            &-spanTe {
                color: #202020;
                font-size: .24rem;
                margin-right: .51rem;
            }
        }
    }
    .changeBtn {
        display: block;
        border: 0;
        width: 6.82rem;
        height: 0.8rem;
        border-radius: 0.6rem;
        @include myself_btn();
        margin: 0 auto;
        margin-top: 0.71rem;
        outline: none;
        box-shadow: 3px 6px 9px #dfa9ae;
    }
    .changeBtn:active {
        transform: scale(.95);
    }
    .imgIcon {
        display: inline-block;
        margin-left: 3.16rem;
        vertical-align: middle;
        width: .18rem;
        height: .18rem;
    }
    .phoneBox {
        width: 5.98rem;
        position: absolute;
        left: 0;
        right: 0;
        top: 3.68rem;
        bottom: 0;
        margin: auto;
        &-p {
            width: 100%;
            font-size: .2rem;
            line-height: .68rem;
            background: #cccccc;
            height: .68rem;
            &-span {
                float: left;
                margin-left: .2rem;
                line-height: .68rem;
            }
            &-img {
                float: right;
                margin-right: .22rem;
                margin-top: .2rem;
                width: .27rem;
                height: .27rem;
            }
        }
        &-div {
            width: 100%;
            height: 2.14rem;
            background: #fff;
            &-botton {
                margin: 0 auto;
                margin-top: .4rem;
                width: 2.18rem;
                height: .64rem;
                background: linear-gradient(to right, #e73a51, #dc3849);
                border: 0;
                display: block;
                color: #fff;
                border-radius: .4rem;
                outline: none;
                box-shadow: 1px 4px 6px #e6c6c9;
                font-size: .24rem;
            }
            &-p {
                margin: 0 auto;
                padding-top: .43rem;
                width: 5.18rem;
                font-size: .2rem;
                line-height: .6rem;
                height: .6rem;
                &-span {
                    float: left;
                    color: #7b7b7b;
                    line-height: .6rem;
                }
                &-input {
                    float: right;
                    width: 4.24rem;
                    height: .5rem;
                    border: 1px solid #a8a8a8;
                }
            }
        }
    }
    .addressBox {
        width: 5.98rem;
        position: absolute;
        left: 0;
        right: 0;
        top: 3.68rem;
        bottom: 0;
        margin: auto;
        &-p {
            width: 100%;
            font-size: .2rem;
            line-height: .68rem;
            background: #cccccc;
            height: .68rem;
            &-span {
                float: left;
                margin-left: .2rem;
                line-height: .68rem;
            }
            &-img {
                float: right;
                margin-right: .22rem;
                margin-top: .2rem;
                width: .27rem;
                height: .27rem;
            }
        }
        &-div {
            width: 100%;
            height: 3.1rem;
            background: #fff;
            &-botton {
                margin: 0 auto;
                margin-top: .4rem;
                width: 2.18rem;
                height: .64rem;
                background: linear-gradient(to right, #e73a51, #dc3849);
                border: 0;
                display: block;
                color: #fff;
                border-radius: .4rem;
                outline: none;
                box-shadow: 1px 4px 6px #e6c6c9;
                font-size: .24rem;
            }
            &-p {
                margin: 0 auto;
                padding-top: .43rem;
                width: 5.18rem;
                font-size: .2rem;
                line-height: .6rem;
                height: .6rem;
                &-span {
                    float: left;
                    color: #7b7b7b;
                    line-height: .6rem;
                }
                &-input {
                    float: right;
                    width: 4rem;
                    height: .5rem;
                    border: 1px solid #a8a8a8;
                }
            }
            &-p1 {
                margin: 0 auto;
                padding-top: .3rem;
                width: 5.18rem;
                font-size: .2rem;
                line-height: .6rem;
                height: .6rem;
                &-span {
                    float: left;
                    color: #7b7b7b;
                    line-height: .6rem;
                }
                &-input {
                    float: right;
                    width: 4rem;
                    height: .5rem;
                    border: 1px solid #a8a8a8;
                }
            }
        }
    }
    *:not(input) {
        -webkit-user-select: none
    }
}