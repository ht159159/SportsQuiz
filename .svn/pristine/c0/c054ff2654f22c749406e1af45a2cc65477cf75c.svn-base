.rank-match {
  height: 100%;
  overflow-x: hidden;
  background: url('~@/assets/matchbgc.png') no-repeat center center fixed;
  background-size: 100%;

  .match {
    overflow-x: hidden;
    height: calc(100% - 1rem);
    .rank-match-head {
      @include page_header_color($theme-red);
    }
  }
  .main-main {
    padding-top: 0.09rem /* 9/100 */;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
  }
  .rank-match-head {
    margin-bottom: 0.72rem /* 72/100 */;
    position: relative;
    height: 1.1rem /* 152/100 */;
    @include page_header_color($theme-red);
    .back-icon {
      width: 0.25rem /* 42/100 */ !important;
      height: 0.43rem /* 42/100 */ !important;
      display: inline-block;
      background-image: url('~@/assets/icons/back.png');
      background-size: 100%;
      margin-left: 0.31rem /* 31/100 */;
      margin-top: 0.35rem;
    }
    .title {
      text-align: center;
      white-space: normal;
      width: 80%;
      box-sizing: border-box;
      margin: 0 auto;
      color: #f2f2f2;
      font-size: 0.33rem; /* 33/100 */
      position: absolute;
      left: 50%;
      top: 29%;
      transform: translateX(-50%);
      white-space: nowrap;
      overflow: auto;
      line-height: 0.33rem;
    }
    .num {
      color: #fed750;
      left: 50%;
      bottom: 17%;
      position: absolute;
      transform: translateX(-50%);
    }
    .count-down-wrap {
      width: 0.6rem;
      height: 0.6rem;
      background: url('~@/assets/countdown.png') no-repeat;
      color: #fff;
      display: flex;
      position: absolute;
      top: 0.25rem;
      right: 0.2rem;
      display: flex;
      justify-content: center;
      align-items: center;
      background-position: 0.02rem -0.02rem;
      .countdown-second {
        
      }
    }
    .myrank {
      text-align: center;
      position: absolute;
      left: 1.18rem;
      bottom: -0.5rem;
    }
    .gold {
      text-align: center;
      position: absolute;
      right: 1.35rem;
      bottom: -0.5rem;
    }
    .ptop {
      font-size: 0.3rem /* 30/100 */;
      color: white;
      line-height: 0.33rem /* 0.33/100 */;
      margin-bottom: 0.05rem /* 5/100 */;
    }
    .pcontent {
      font-size: 0.22rem /* 22/100 */;
      color: #ffc700;
      font-weight: 700;
    }
  }
  .rank-match-main {
    width: 7.1rem /* 710/100 */;
    margin: 0 auto;
    margin-bottom: 0.3rem /* 30/100 */;
    .title {
      width: 100%;
      height: 0.63rem /* 62/100 */;
      background: url('~@/assets/match-title.png') no-repeat;
      background-size: 100%;
      position: relative;
      .title-left {

      }
      .title-center {

      }
      .title-right {

      }
      .commondiv {
        width: 0.68rem /* 58/100 */;
        height: 0.58rem /* 58/100 */;
        border-radius: 0.03rem /* 3/100 */;
        text-align: center;
        line-height: 0.58rem /* 0.58/100 */;
        color: #fff;
        font-weight: 500;
      }
      .home {
        background-color: #ea0546;
      }
      .away {
        background-color: #129829;
      }
      .homename {
        font-size: 0.32rem /* 32/100 */;
        color: #8fa5de;
        line-height: 0.62rem;
        margin-left: 0.1rem /* 10/100 */;
        white-space: nowrap;
        overflow: auto;
      }
      .awayname {
        font-size: 0.32rem /* 32/100 */;
        color: #8fa5de;
        line-height: 0.62rem;
        margin-right: 0.1rem /* 10/100 */;
        white-space: nowrap;
        overflow: auto;
      }
    }

    .banner1 {
      box-sizing: border-box;
      margin-top: 0.12rem /* 12/100 */;
      width: 100%;
      height: 0.53rem /* 53/100 */;
      background: url('~@/assets/match-main1.png');
      background-size: 100%;

      &-quset {
        display: inline-block;
        width: 0.43rem /* 43/100 */;
        height: 0.43rem /* 43/100 */;
        background: url('~@/assets/questyellow.png');
        background-size: 100%;
        float: right;
      }
      &-tabs {
        box-sizing: border-box;
        position: relative;
        .tab {
          position: absolute;
          box-sizing: border-box;
          float: left;
          width: 2.2rem /* 186/100 */;
          height: 0.53rem /* 53/100 */;
          background-color: #2c3650;
          border-top-left-radius: 0.07rem /* 7/100 */;
          border-top-right-radius: 0.07rem /* 7/100 */;
          border: 1px solid #3a4666;
          bottom: -0.53rem;
          text-align: center;
          line-height: 0.53rem /* 0.53/100 */;
          color: #62739f;
        }
        .tab:nth-child(2) {
          left: 2rem /* 163/100 */;
          z-index: 1;
        }
        .tab:nth-child(3) {
          left: 4.1rem /* 326/100 */;
        }
        .tab-active {
          width: 2.3rem /* 187/100 */;
          height: 0.58rem /* 58/100 */;
          background-color: #facf3b;
          z-index: 10;
          color: #161525;
        }
      }
    }
    .guess-main {
      .pstyle {
        color: #ffc700 !important;
      }
      width: 7.1rem /* 710/100 */;
      height: 5.2rem /* 520/100 */;
      background-color: rgba(0, 0, 0, 0.5);
      &-title {
        height: 0.67rem /* 67/100 */;
        display: flex;
        justify-content: space-between;
        align-items: center;
        .left {
          margin-top: 0.15rem /* 15/100 */;
          padding-left: 0.19rem /* 19/100 */;
          line-height: 0.67rem;
          width: 33.3%;
          float: left;
          p:nth-child(1) {
            color: #cbd2e2;
            font-size: 0.2rem; /* 20/100 */
          }
          p:nth-child(2) {
            color: #129827;
            font-size: 0.18rem /* 18/100 */;
          }
        }
        .time {
          width: 2.5rem;
          float: left;
          color: #a3b7ec;
          font-size: 0.22rem /* 22/100 */;
          line-height: 0.67rem;
        }
        .more {
          // margin-top: 0.19rem /* 19/100 */;
          // margin-right: 0.19rem /* 0.19/100 */;
          width: 33.3%;
          float: right;
          width: 1.07rem /* 107/100 */;
          height: 0.35rem /* 35/100 */;
          background-color: #fff;
          background: url('~@/assets/button_03.png');
          background-size: 100%;
          font-size: 0.18rem /* 20/100 */;
          color: #5e2f13;
          text-align: center;
          line-height: 0.35rem;
        }
      }
      &-content {
        box-sizing: border-box;
        margin: 0 auto;
        width: 6.71rem /* 671/100 */;
        height: 4.33rem /* 433/100 */;
        //background-color: rgba(22, 31, 74, 0.6);
        border-radius: 0.04rem /* 4/100 */;
        //padding: 0.28rem /* 28/100 */ 0.16rem /* 16/100 */;
        .content-item div:first-child{
          overflow-x: scroll;
        }
        .content-item {
          height: 0.6rem /* 53/100 */;
          background-color: rgba(22, 31, 74, 0.6);
          div {
            width: 25%;
            float: left;
            text-align: center;
            line-height: 0.53rem;
            font-size: 0.26rem /* 26/100 */;
            color: #e0e0de;
            // height: 0.22rem /* 22/100 */;
          }
        }
        .content-bitem {
          height: 1.09rem /* 109/100 */;

          div {
            width: 25%;
            float: left;
            text-align: center;
            line-height: 1.09rem;
            font-size: 0.26rem /* 26/100 */;
            color: #e0e0de;
          }
        }
      }
    }
  }
  position: relative;
  .match-bottom {
    // position: absolute;
    // bottom: 0;
  }
  .IsOddsChangeBtn {
    width: 3.55rem;
    height: 0.8rem;
    background: var(--primary-color);
    border: none;
    font-size: 0.32rem;
    line-height: 0.8rem;
    border-radius: 40px;
    color: #ffffff;
    display: block;
    margin: 0 auto;
    -webkit-transition: 0.3s;
    transition: 0.3s;
    text-align: center;
    box-shadow: 6px 6px 6px #8c7d7f;
  }
}
