.ranklist {
  &-head {
    height: 2.21rem /* 221/100 */;
    width: 100%;
    // background-image: url('~@/assets/ranklisthead.png');
    background: url('~@/assets/ranklisthead.png') no-repeat center center;
    background-size: 100%;
    .back-icon {
      width: 0.42rem /* 42/100 */;
      height: 0.42rem /* 42/100 */;
      display: inline-block;
      background-image: url('~@/assets/icons/back.png');
      background-size: 100%;
      margin-left: 0.31rem /* 31/100 */;
      margin-top: 0.5rem;
      float: left;
    }
    position: relative;
    p {
      position: absolute;
      color: white;
      // margin-top: 0.78rem; /* 23/100 */

      font-size: 0.24rem /* 24/100 */;
      font-weight: 600;
      left: 50%;
      bottom: 0.23rem /* 23/100 */;
      transform: translateX(-50%);
    }
  }
  &-main {
    height: 11.13rem /* 1113/100 */;
    padding-top: 0.32rem /* 32/100 */;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    // 列表
    .list-content {
      background-color: #fff;
      position: relative;
      box-sizing: border-box;
      width: 7.1rem /* 710/100 */;
      height: 1.11rem /* 111/100 */;
      //   background-color: red;
      margin-left: 0.2rem /* 20/100 */;
      margin-bottom: 0.3rem /* 19/100 */;
      border-radius: 5.05rem;
      border: 2px solid #d5c8a9;
      .round {
        float: left;

        width: 0.83rem /* 83/100 */;
        height: 0.83rem /* 83/100 */;
        background-color: #fff;
        border-radius: 0.42rem;
        margin-left: 0.13rem /* 13/100 */;
        margin-top: 0.1rem /* 10/100 */;
        font-size: 0.46rem /* 46/100 */;
        line-height: 0.83rem;
        text-align: center;
        color: #aaaaaa;
      }
      .list-date {
        span {
          font-size: 0.3rem /* 30/100 */;
          margin-top: 0.24rem; /* 24/100 */
          margin-left: 0.17rem /* 17/100 */;
          display: inline-block;
        }
        .list-arrow {
          width: 0.18rem /* 18/100 */;
          height: 0.18rem;
          margin-left: 0.18rem;
          display: inline-block;
          background-image: url('~@/assets/icons/arrow1.png');
          background-size: 100%;
        }
      }
      .list-info {
        margin-top: 0.1rem /* 20/100 */;

        span {
          font-size: 0.16rem /* 16/100 */;

          color: #9f9f9f;
          font-size: 0.2rem /* 20/100 */;
          margin-right: 0.17rem;
        }
        span:first-child {
          margin-left: 0.17rem /* 17/100 */;
        }
      }
      .start {
        position: absolute;
        right: 0.4rem;
        top: 0rem;
        height: 100%;
        width: 1.25rem /* 125/100 */;
        // background-color: red;
      }
    }
  }
}
